{
    "version": 2,
    "builds": [
      {
        "src": "src/scripts/App.py",
        "use": "@vercel/python"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      { "src": "/api/.*", "dest": "src/scripts/App.py" },
      { "src": "/(.*)", "dest": "/build/$1" }
    ]
  }
  